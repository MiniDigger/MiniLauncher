<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.131"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.minidigger.minecraftlauncher.launcher.LauncherOptionsController">
    <BorderPane prefHeight="500.0" prefWidth="400.0">
        <top>
            <Pane prefHeight="25.0" prefWidth="500.0" styleClass="top_title_bar" BorderPane.alignment="CENTER">
                <Button fx:id="optionsExit" layoutX="367.0" layoutY="4.0" maxHeight="18.0" maxWidth="18.0"
                        minHeight="18.0" minWidth="18.0" mnemonicParsing="false" onAction="#_optionsExit"
                        styleClass="exit_button"/>
                <Pane layoutX="385.0" prefHeight="25.0" prefWidth="15.0"/>
                <Pane prefHeight="25.0" prefWidth="15.0"/>
                <Label layoutX="15.0" layoutY="4.0" styleClass="label-black"
                       text="Minecraft Launcher - Options"/>
            </Pane>
        </top>
        <bottom>
            <Pane prefHeight="35.0" prefWidth="400.0" styleClass="top_title_bar" BorderPane.alignment="CENTER">
                <Button fx:id="optionsClose" layoutX="321.0" layoutY="5.0" mnemonicParsing="false"
                        onAction="#_optionsClose" prefHeight="25.0" prefWidth="65.0" text="Close"/>
                <Label fx:id="optionStatus" layoutX="15.0" layoutY="9.0" styleClass="label-black"
                       text="Status: Idle"/>
            </Pane>
        </bottom>
        <left>
            <Pane prefHeight="200.0" prefWidth="5.0" styleClass="top_title_bar" BorderPane.alignment="CENTER"/>
        </left>
        <right>
            <Pane prefHeight="200.0" prefWidth="5.0" styleClass="top_title_bar" BorderPane.alignment="CENTER"/>
        </right>
        <center>
            <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
                <Pane prefHeight="108.0" prefWidth="390.0">
                    <Label layoutX="10.0" layoutY="2.0" text="Launcher Settings">
                        <font>
                            <Font size="14.0"/>
                        </font>
                    </Label>
                    <RadioButton fx:id="optionsKeepLauncherOpen" layoutX="13.0" layoutY="25.0"
                                 mnemonicParsing="false" onAction="#_optionsKeepLauncherOpen"
                                 text="Keep Launcher Open">
                        <tooltip>
                            <Tooltip fx:id="tt_keepLauncherOpen" prefWidth="300.0" text="Empty Tooltip"
                                     wrapText="true"/>
                        </tooltip>
                    </RadioButton>
                    <RadioButton fx:id="optionDisableAutoUpdates" disable="true" layoutX="13.0"
                                 layoutY="50.0" mnemonicParsing="false"
                                 onAction="#_optionDisableAutoUpdates" text="Disable Auto Updates"/>
                    <ComboBox fx:id="themeType" disable="true" layoutX="226.0" layoutY="71.0"
                              onAction="#_themeType" prefHeight="25.0" prefWidth="150.0"
                              promptText="Choose a theme"/>
                    <RadioButton fx:id="useThemeType" layoutX="13.0" layoutY="75.0" mnemonicParsing="false"
                                 onAction="#_useThemeType" text="Custom Theme">
                        <tooltip>
                            <Tooltip fx:id="tt_customTheme" prefWidth="300.0" text="Empty Tooltip"
                                     wrapText="true"/>
                        </tooltip>
                    </RadioButton>
                    <Label fx:id="launcherVersion" layoutX="330.0" layoutY="6.0"
                           onMouseClicked="#mc_launcherVersion" text="Version: 0.00">
                        <font>
                            <Font size="8.0"/>
                        </font>
                        <tooltip>
                            <Tooltip fx:id="tt_launcherVersion" prefWidth="300.0" text="Empty Tooltip"
                                     wrapText="true"/>
                        </tooltip>
                    </Label>
                </Pane>
                <Pane prefHeight="3.0" prefWidth="390.0" styleClass="top_title_bar"/>
                <Pane prefHeight="108.0" prefWidth="200.0">
                    <Label layoutX="10.0" layoutY="2.0" text="Minecraft Settings">
                        <font>
                            <Font size="14.0"/>
                        </font>
                    </Label>
                    <RadioButton fx:id="optionsResolution" layoutX="10.0" layoutY="25.0"
                                 mnemonicParsing="false" onAction="#_optionsResolution" text="Resolution:">
                        <tooltip>
                            <Tooltip fx:id="tt_resolution" prefWidth="300.0" text="Empty Tooltip"
                                     wrapText="true"/>
                        </tooltip>
                    </RadioButton>
                    <RadioButton fx:id="optionsRamAllocation" layoutX="10.0" layoutY="50.0"
                                 mnemonicParsing="false" onAction="#_optionsRamAllocation"
                                 text="Ram Allocation:">
                        <tooltip>
                            <Tooltip fx:id="tt_ramAllocation" prefWidth="300.0" text="Empty Tooltip"
                                     wrapText="true"/>
                        </tooltip>
                    </RadioButton>
                    <RadioButton fx:id="optionsBypassBlacklist" layoutX="10.0" layoutY="75.0"
                                 mnemonicParsing="false" onAction="#_optionsBypassBlacklist"
                                 text="Bypass Blacklist">
                        <tooltip>
                            <Tooltip fx:id="tt_bypassBlacklist" prefWidth="300.0" text="Empty Tooltip"
                                     wrapText="true"/>
                        </tooltip>
                    </RadioButton>
                    <TextField fx:id="optionsResolutionMin" disable="true" layoutX="144.0" layoutY="24.0"
                               maxHeight="20.0" minHeight="20.0" onKeyTyped="#kt_optionsResolutionMin"
                               prefHeight="20.0" prefWidth="103.0" promptText="Width"/>
                    <TextField fx:id="optionsRamAllocationMin" disable="true" layoutX="144.0" layoutY="50.0"
                               maxHeight="20.0" minHeight="20.0" onKeyTyped="#kt_optionsRamAllocationMin"
                               prefHeight="20.0" prefWidth="103.0" promptText="Min"/>
                    <TextField fx:id="optionsResolutionMax" disable="true" layoutX="273.0" layoutY="24.0"
                               maxHeight="20.0" minHeight="20.0" onKeyTyped="#kt_optionsResolutionMax"
                               prefHeight="20.0" prefWidth="103.0" promptText="Height"/>
                    <TextField fx:id="optionsRamAllocationMax" disable="true" layoutX="273.0" layoutY="50.0"
                               maxHeight="20.0" minHeight="20.0" onKeyTyped="#kt_optionsRamAllocationMax"
                               prefHeight="20.0" prefWidth="103.0" promptText="Max"/>
                </Pane>
                <Pane prefHeight="3.0" prefWidth="390.0" styleClass="top_title_bar"/>
                <Pane prefHeight="108.0" prefWidth="390.0">
                    <Label layoutX="10.0" layoutY="2.0" text="Version Settings">
                        <font>
                            <Font size="14.0"/>
                        </font>
                    </Label>
                    <ComboBox fx:id="optionsSelectVersion" layoutX="10.0" layoutY="25.0" minHeight="25.0"
                              onAction="#_optionsSelectVersion" prefHeight="25.0" prefWidth="285.0"
                              promptText="Select a version to download &amp; install">
                        <tooltip>
                            <Tooltip fx:id="tt_selectVersion" prefWidth="300.0" text="Empty Tooltip"
                                     wrapText="true"/>
                        </tooltip>
                    </ComboBox>
                    <Button fx:id="optionsSelectVersionInstall" layoutX="311.0" layoutY="25.0"
                            mnemonicParsing="false" onAction="#_optionsSelectVersionInstall"
                            prefHeight="25.0" prefWidth="65.0" text="Install">
                        <tooltip>
                            <Tooltip fx:id="tt_selectVersionInstall" prefWidth="300.0" text="Empty Tooltip"
                                     wrapText="true"/>
                        </tooltip>
                    </Button>
                    <RadioButton fx:id="optionsSelectVersionForce" layoutX="10.0" layoutY="55.0"
                                 mnemonicParsing="false" onAction="#_optionsSelectVersionForce"
                                 text="Force Download">
                        <tooltip>
                            <Tooltip fx:id="tt_forceDownload" prefWidth="300.0" text="Empty Tooltip"
                                     wrapText="true"/>
                        </tooltip>
                    </RadioButton>
                    <RadioButton fx:id="optionsSelectFastStart" layoutX="10.0" layoutY="77.0"
                                 mnemonicParsing="false" onAction="#_optionsSelectFastStart"
                                 text="Fast Start">
                        <tooltip>
                            <Tooltip fx:id="tt_fastStartup" prefWidth="300.0" text="Empty Tooltip"
                                     wrapText="true"/>
                        </tooltip>
                    </RadioButton>
                </Pane>
                <Pane prefHeight="3.0" prefWidth="390.0" styleClass="top_title_bar"/>
                <Pane prefHeight="108.0" prefWidth="390.0">
                    <Label layoutX="10.0" layoutY="2.0" text="Java Settings" textOverrun="CLIP">
                        <font>
                            <Font size="14.0"/>
                        </font>
                    </Label>
                    <RadioButton fx:id="optionsJavaVersion" layoutX="10.0" layoutY="25.0"
                                 mnemonicParsing="false" onAction="#_optionsJavaVersion"
                                 text="Java Version:">
                        <tooltip>
                            <Tooltip fx:id="tt_javaVersion" prefWidth="300.0" text="Empty Tooltip"
                                     wrapText="true"/>
                        </tooltip>
                    </RadioButton>
                    <RadioButton fx:id="optionsJVMArguments" layoutX="10.0" layoutY="50.0"
                                 mnemonicParsing="false" onAction="#_optionsJVMArguments"
                                 text="JVM Arguments:">
                        <tooltip>
                            <Tooltip fx:id="tt_jvmArgs" prefWidth="300.0" text="Empty Tooltip"
                                     wrapText="true"/>
                        </tooltip>
                    </RadioButton>
                    <TextField fx:id="optionsJavaVersionInput" disable="true" layoutX="139.0" layoutY="24.0"
                               maxHeight="20.0" minHeight="20.0"
                               onMousePressed="#mp_optionsJavaVersionInput" prefHeight="20.0"
                               prefWidth="237.0"/>
                    <TextField fx:id="optionsJVMArgumentsInput" disable="true" layoutX="139.0"
                               layoutY="49.0" maxHeight="20.0" minHeight="20.0" prefHeight="20.0"
                               prefWidth="237.0"/>
                    <RadioButton fx:id="optionsDebugMode" layoutX="10.0" layoutY="75.0"
                                 mnemonicParsing="false" onAction="#_optionsDebugMode" text="Debug Mode">
                        <tooltip>
                            <Tooltip fx:id="tt_debugMode" prefWidth="300.0" text="Empty Tooltip"
                                     wrapText="true"/>
                        </tooltip>
                    </RadioButton>
                </Pane>
            </VBox>
        </center>
    </BorderPane>
</AnchorPane>
